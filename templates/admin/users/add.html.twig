{% extends '/admin/dashboard.html.twig' %}
{# ~ permet de concaterner #}

{% block title "Utilisateur " ~  "" %}

{% block admin %}
    <h2>Utilisateur nouveau</h2>
    <div class="row">
        {{ form_start(form) }}
            
            {{ form_row(form.username) }}
            {{ form_row(form.email) }}
            {{ form_row(form.password) }}
        
        
            {{ form_row(form.roles) | raw }}
        
            {{ form_row(form.isVerified) }}
         

        {{ form_end(form) }}

    </div>
{% endblock %}